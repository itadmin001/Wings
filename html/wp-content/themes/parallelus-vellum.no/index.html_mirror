<script>

	var spacer_group, metal_d_ring, res_handle, hres_group, Hacky, FF_Handle, BOC, pinstripes_3, pinstripes_5, ColorMainStripe;
	var display_diagram = document.getElementById("diag_show_hide");
	var display_spacer_foam = document.getElementById("spacer_show_hide");
	var metal_d_ring_option = document.getElementById("res_metal_dring");
	var deployment_option = document.getElementById("main-release");
	var pinstripe_option = document.getElementById("add-pinstripes");
	var mainHolder, colorHolder;
	var btnClear, btnDownloadSVG, btnDownloadPNG;
